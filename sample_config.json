{
  "services": {
    "github_public": {
      "base_url": "https://api.github.com",
      "requires_service_token": false,
      "_comment": "base_url can be 'api.github.com' (defaults to https) or explicit 'https://api.github.com'"
    },
    "github_public_behind_service_token": {
      "base_url": "https://api.github.com",
      "requires_service_token": true
    },
    "github_private_behind_service_token": {
      "base_url": "https://api.github.com",
      "credential": {
        "type": "bearer",
        "token": "YOUR_GITHUB_API_TOKEN"
      },
      "requires_service_token": true,
      "valid_audiences": ["zapier_webhook"]
    },
    "long_running_service_example": {
      "base_url": "https://api.example.com",
      "requires_service_token": true,
      "connect_timeout": 10,
      "read_timeout": 300,
      "allow_redirects": true
    }
  },
  "audit_log": {
    "type": "local_dir",
    "root_dir": "/var/tmp/lockbox_audit_log"
  }
}
